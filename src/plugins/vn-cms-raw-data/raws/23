 window.isCpaMobile = {
 Android: function () {
 if (navigator.userAgent.match(/Android/i) == null)
 return [];
 return navigator.userAgent.match(/Android/i)
 },
 BlackBerry: function () {
 if (navigator.userAgent.match(/BlackBerry|BB|PlayBook/i) == null)
 return [];
 return navigator.userAgent.match(/BlackBerry|BB|PlayBook/i)
 },
 iOS: function () {
 if (navigator.userAgent.match(/iPhone|iPad|iPod/i) == null)
 return [];
 return navigator.userAgent.match(/iPhone|iPad|iPod/i)
 },
 Opera: function () {
 if (navigator.userAgent.match(/Opera Mini|Opera Mobi/i) == null)
 return [];

 return navigator.userAgent.match(/Opera Mini|Opera Mobi/i)
 },
 Windows: function () {
 if (navigator.userAgent.match(/Windows Phone|IEMobile/i) == null)
 return [];

 return navigator.userAgent.match(/Windows Phone|IEMobile/i)
 },
 any: function () {
 if (this.Android().length != 0 || this.BlackBerry().length != 0 || this.iOS().length != 0 || this.Opera().length != 0 || this.Windows().length != 0)
 return true;
 else return false;
 },
 valueOS: function () {
 return this.Android() ? 1 : this.iOS() ? 2 : this.Windows() ? 3 : this.BlackBerry() ? 6 : ""
 }
 }

